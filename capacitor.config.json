{
  "appId": "com.mykochionline.apptester",
  "appName": "MyKochi-app",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  },
  "server": {
    "iosScheme": "ionic",
    "androidScheme": "http",
    "allowNavigation": [
      "*.mykochionline.com",
      "*.mykochionline.com/wp-json/wp/v2"
    ]
  },
  "android": {
    "webContentsDebuggingEnabled": true
  },
  "cordova": {}
}